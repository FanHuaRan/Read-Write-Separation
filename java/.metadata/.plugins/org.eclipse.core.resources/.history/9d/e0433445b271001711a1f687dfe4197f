package com.fhr.osmonitor.utils;
/**
 * 资源辅助类
 * @author ASUS
 * @date 2017/07/26
 */
public class ResourceUtils {
	public static String getAbsoluteFileName(String resourceFileName){
		String absoluteFile = Thread.currentThread().getContextClassLoader().getResource(resourceFileName)
				.getFile();
		return absoluteFile;
	}
}
